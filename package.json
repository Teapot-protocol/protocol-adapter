{
  "name": "protocol-adapter",
  "version": "1.0.0",
  "description": "Universal protocol adaptation system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "protocol",
    "adapter",
    "transformation",
    "conversion",
    "interoperability"
  ],
  "author": "Teapot Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Teapot-protocol/protocol-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/Teapot-protocol/protocol-adapter/issues"
  },
  "homepage": "https://github.com/Teapot-protocol/protocol-adapter#readme"
}